{
  "functions": {
    "source": ".output/server",
    "runtime": "nodejs20"
  },
  "hosting": {
    "site": "psm-mcq",
    "public": ".output/public",
    "cleanUrls": true,
    "rewrites": [
      {
        "source": "**",
        "function": "server",
        "region": "asia-south1"
      }
    ],
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ]
  }
}